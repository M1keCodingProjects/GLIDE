use "std GLib" with std
make tape = "                                                                           *" spill

replace checkBin with "*" == 4 * swap "*" == 2 * + swap "*" == +
replace rule110  with 110 swap >> 1 &

loop tape range 2 // {
  make tape2 = "*"
  loop tape range 2 - with i {
    tape2 += ?( tape[ i ] tape[ i 1 + ] tape[ i 2 + ] checkBin rule110 pop "*" " " )
  }
  tape = tape2 spill
  print tape spill str
}