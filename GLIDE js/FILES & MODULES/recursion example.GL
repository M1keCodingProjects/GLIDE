use "std GLib" with std

def fact => n {
  when n 2 < { exit 1 }
  exit n fact( n 1 - ) *
}

def fib => n {
  when n 2 < { exit 1 }
  exit fib( n 1 - ) fib( n 2 - ) +
}

def sumAllNext => arr sum {
  when arr range not { exit "execution success" }
  sum += arr[ arr range 1 - ]
  sumAllNext( arr pack spill drop pack sum )
  print sum
}

sumAllNext( 12 23 45 22 9 2 18 pack 0 )